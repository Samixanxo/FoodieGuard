<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_graph"
    app:startDestination="@id/home_fragment">
    <fragment
        android:id="@+id/home_fragment"
        android:name="com.example.foodie_guardv0.Activity.home_fragment"
        android:label="home_fragment"
        tools:layout="@layout/fragment_home_fragment"
        >
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/search_Fragment" />
        <action
            android:id="@+id/action_homeFragment_to_homeFragment"
            app:destination="@id/home_fragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/user" />
    </fragment>
    <fragment
        android:id="@+id/user"
        android:name="com.example.foodie_guardv0.Activity.user_fragment"
        android:label="user"
        tools:layout="@layout/fragment_user"
        >
        <action
            android:id="@+id/travelUserFragment"
            app:destination="@id/user" />
    </fragment>
    <fragment
        android:id="@+id/search_Fragment"
        android:name="com.example.foodie_guardv0.Activity.search_Fragment"
        android:label="search_Fragment"
        tools:layout="@layout/fragment_search_"
        />
</navigation>